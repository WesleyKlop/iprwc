{
  "name": "rest-api",
  "version": "0.0.0",
  "packageManager": "npm@8.1.2",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "DEBUG=rest-api:* node src/bin/server.mjs",
    "prepare": "husky install",
    "lint:fix": "prettier --write src prisma",
    "lint:check": "prettier --check src prisma"
  },
  "dependencies": {
    "@prisma/client": "^3.5.0",
    "bcrypt": "^5.0.1",
    "debug": "~2.6.9",
    "dotenv": "^10.0.0",
    "express": "~4.16.1",
    "jose": "^4.3.7",
    "morgan": "~1.9.1",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.13",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.2",
    "prettier": "^2.4.1",
    "prisma": "^3.5.0"
  },
  "lint-staged": {
    "*.{js,mjs,md}": "prettier --write",
    "*.prisma": "prisma format"
  },
  "prisma": {
    "seed": "node prisma/seed.mjs"
  }
}
