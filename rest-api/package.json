{
  "name": "rest-api",
  "version": "0.0.0",
  "packageManager": "npm@8.1.2",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "DEBUG=rest-api:* node src/bin/server.mjs",
    "prepare": "cd .. && husky install rest-api/.husky",
    "lint:fix": "prettier --write src prisma && eslint --fix src",
    "lint:check": "prettier --check src prisma && eslint src"
  },
  "dependencies": {
    "@prisma/client": "^3.5.0",
    "bcrypt": "^5.0.1",
    "debug": "~2.6.9",
    "dotenv": "^10.0.0",
    "express": "^5.0.0-alpha.8",
    "jose": "^4.3.7",
    "morgan": "~1.9.1",
    "multer": "^1.4.3",
    "uuid": "^8.3.2",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.16.3",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.13",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.2",
    "prettier": "^2.4.1",
    "prisma": "^3.5.0"
  },
  "lint-staged": {
    "*.{js,mjs,md}": "prettier --write",
    "*.prisma": "prisma format"
  },
  "prisma": {
    "seed": "node prisma/seed.mjs"
  }
}
